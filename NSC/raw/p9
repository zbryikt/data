<input type="hidden" name="wUctlAwardQueryPage$hidPage" id="wUctlAwardQueryPage_hidPage" />

<script type="text/javascript">
        function CheckedAll(objCheck, objResponse) {
            var table = document.getElementById(objResponse);

            for (i = 0; i < table.rows.length; i++) {
                for (j = 0; j < table.rows[i].cells.length; j++)
                    table.rows[i].cells[j].childNodes[0].checked = document.getElementById(objCheck).checked;
            }
        }
        function DisCheckedAll(objCheck, objResponse) {
            var table = document.getElementById(objCheck);
            var intChecked = 0;
            var intNum = 0
            for (i = 0; i < table.rows.length; i++) {
                for (j = 0; j < table.rows[i].cells.length; j++) {
                    if (table.rows[i].cells[j].childNodes[0].checked == true) 
                        intChecked += 1;
                    intNum += 1;
                }
            }
            document.getElementById(objResponse).checked = (intChecked != intNum ? false : true)
        }
</script>


                                                
